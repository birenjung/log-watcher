{
  "name": "birenjung/log-watcher",
  "description": "Terminal-first, channel-aware log watcher for Laravel developers",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "logs",
    "logging",
    "artisan",
    "developer-tools",
    "terminal"
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "symfony/console": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Birenjung\\LogWatcher\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Birenjung\\LogWatcher\\LogWatcherServiceProvider"
      ]
    }
  },
  "support": {
    "issues": "https://github.com/birenjung/log-watcher/issues",
    "source": "https://github.com/birenjung/log-watcher"
  }
}
