{
  "name": "birenjung/log-watcher",
  "description": "Dev-only Laravel log watcher with colored output",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "symfony/console": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Birenjung\\LogWatcher\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Birenjung\\LogWatcher\\LogWatcherServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
